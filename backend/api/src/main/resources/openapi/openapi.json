{
  "openapi": "3.0.0",
  "info": {
    "title": "InstantDomains API",
    "description": "InstantDomains API Service",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.getinstantdomains.com",
      "description": "Prod"
    },
    {
      "url": "http://localhost:6868",
      "description": "Dev"
    }
  ],
  "paths": {
    "/leads/find": {
      "post": {
        "tags": [
          "lead"
        ],
        "summary": "Submit search query",
        "operationId": "submitSearchQuery",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SubmitQueryRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Task id and status",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TaskId"
                }
              }
            }
          }
        }
      }
    },
    "/leads/all": {
      "post": {
        "tags": [
          "lead"
        ],
        "summary": "Get all leads",
        "operationId": "getAllUserLeads",
        "responses": {
          "200": {
            "description": "All leads",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EmailSearchResults"
                }
              }
            }
          }
        }
      }
    },
    "/lead/youtube/channels/result": {
      "get": {
        "tags": [
          "lead"
        ],
        "summary": "Get channel query result",
        "operationId": "getChannelQueryResult",
        "parameters": [
          {
            "in": "query",
            "name": "job_id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Channel info response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ChannelPageableResponse"
                }
              }
            }
          }
        }
      }
    },
    "/user/": {
      "get": {
        "tags": [
          "user"
        ],
        "summary": "Get user profile",
        "description": "Get user profile or create one if it doesn't exist",
        "operationId": "getUserprofile",
        "responses": {
          "200": {
            "description": "Get user profile response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserProfileResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "TaskId": {
        "type": "object",
        "properties": {
          "task_id": {
            "type": "string"
          },
          "status": {
            "type": "string"
          }
        }
      },
      "SubmitQueryRequest": {
        "type": "object",
        "properties": {
          "query": {
            "type": "string"
          },
          "webpage_only": {
            "type": "boolean"
          }
        }
      },
      "ChannelPageableResponse": {
        "type": "object",
        "required": [
          "count",
          "records"
        ],
        "properties": {
          "count": {
            "type": "integer",
            "format": "int32"
          },
          "records": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/YouTubeChannelDetail"
            }
          }
        }
      },
      "EmailSearchResults": {
        "type": "object",
        "properties": {
          "results": {
            "type": "object",
            "items": {
              "$ref": "#/components/schemas/EmailSearchResult"
            }
          }
        }
      },
      "EmailSearchResult": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "keywords": {
            "type": "string"
          },
          "phone_number": {
            "type": "string"
          },
          "job_title": {
            "type": "string"
          },
          "gender": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "state": {
            "type": "string"
          },
          "industry": {
            "type": "string"
          },
          "company_name": {
            "type": "string"
          },
          "website": {
            "type": "string"
          },
          "linkedin_profile": {
            "type": "string"
          },
          "age": {
            "type": "string"
          },
          "company_size": {
            "type": "string"
          },
          "verified": {
            "type": "boolean"
          },
          "thumbnail": {
            "type": "string"
          }
        }
      },
      "WebsocketPayload": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "data": {
            "type": "object"
          }
        }
      },
      "YouTubeChannelDetail": {
        "type": "object",
        "properties": {
          "query": {
            "type": "string"
          },
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "facebook": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "keywords": {
            "type": "string"
          },
          "channel_url": {
            "type": "string"
          },
          "channel_name": {
            "type": "string"
          },
          "channel_handle": {
            "type": "string"
          },
          "subscribers": {
            "type": "string"
          },
          "subscribers_value": {
            "type": "integer",
            "format": "int64"
          },
          "instagram": {
            "type": "string"
          },
          "twitter": {
            "type": "string"
          },
          "website": {
            "type": "string"
          },
          "blog": {
            "type": "string"
          },
          "snap_chat": {
            "type": "string"
          },
          "discord": {
            "type": "string"
          },
          "tiktok": {
            "type": "string"
          },
          "pinterest": {
            "type": "string"
          }
        }
      },
      "UserProfileResponse": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string"
          },
          "profile": {
            "$ref": "#/components/schemas/UserProfile"
          }
        }
      },
      "GenericResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string"
          },
          "error": {
            "type": "string"
          }
        }
      },
      "UserProfile": {
        "type": "object",
        "properties": {
          "user_id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "picture": {
            "type": "string"
          },
          "api_key": {
            "type": "string"
          },
          "anonymous": {
            "type": "boolean"
          },
          "roles": {
            "type": "object",
            "additionalProperties": {
              "type": "boolean"
            }
          },
          "is_authorized": {
            "type": "boolean"
          },
          "auth_mode": {
            "type": "string"
          },
          "dashboard_data": {
            "$ref": "#/components/schemas/DashboardData"
          }
        }
      },
      "DashboardData": {
        "type": "object",
        "properties": {
          "all_emails": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "PageableRequest": {
        "type": "object",
        "required": [
          "limit",
          "page_num"
        ],
        "properties": {
          "query": {
            "type": "string"
          },
          "limit": {
            "type": "integer",
            "format": "int32"
          },
          "page_num": {
            "type": "integer",
            "format": "int32"
          },
          "sortBy": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "sortDir": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
